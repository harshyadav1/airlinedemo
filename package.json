{
  "name": "travel-flights",
  "version": "0.1.0",
  "description": "[travel] - just pack your bag, we know the way",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server",
    "build": "rimraf dist && webpack --config webpack.prod.config.js --progress --profile --color"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/scripts/test-ignore.js",
      "<rootDir>/scripts/test-setup.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.(jsx|js)?$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "collectCoverageFrom": [
      "**/*.component.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text-summary"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "flight-search",
    "materialui",
    "material"
  ],
  "author": "Ali Sabri GÃ–K <alisabrigok@gmail.com> (http://www.alisabri.com)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "postcss-loader": "^2.0.7",
    "react-test-renderer": "^16.4.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "@material-ui/lab": "^1.0.0-alpha.6",
    "axios": "^0.18.0",
    "material-ui-pickers": "^1.0.0-rc.11",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0-beta.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
